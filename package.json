{
  "name": "paste-it",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev:web": "pnpm -F web dev",
    "build:web": "pnpm -F web build",
    "lint:web": "pnpm -F web lint",
    "preview:web": "pnpm -F web preview",
    "build:server": "pnpm -F server build",
    "dev:server": "pnpm -F server dev",
    "lint:server": "pnpm -F server lint",
    "start": "turbo run start",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "postinstall": "pnpm -F db db:generate"
  },
  "dependencies": {
    "@eslint/js": "^9.22.0",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/static": "^8.1.1",
    "@paste-it/db": "workspace:*",
    "@paste-it/data": "workspace:*",
    "@tanstack/react-query": "^4.36.1",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/fastify": "^3.52.1",
    "@ts-rest/react-query": "^3.52.1",
    "@types/node": "22",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@unocss/reset": "66.1.0-beta.11",
    "@unocss/transformer-directives": "66.1.0-beta.11",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "devalue": "^5.1.1",
    "eslint": "^9.22.0",
