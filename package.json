    "eslint-plugin-react-refresh": "^0.4.19",
    "fastify": "^5.3.0",
    "globals": "^16.0.0",
    "pkgroll": "^2.12.1",
    "prisma": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.1",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "unocss": "66.1.0-beta.11",
    "unocss-preset-animations": "^1.1.1",
    "unplugin-icons": "^22.1.0",
    "vite": "^6.3.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@prisma/client": "6.6.0",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
